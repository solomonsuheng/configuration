#Start Setting (create a new repository on the command line)
touch README.md
git init
git add README.md
git commit -m "first commit"
git remote add origin git@github.com:solomonsuheng/Demo.git
git push -u origin master

#Push an existing repository from the command line
git remote add origin git@github.com:solomonsuheng/Demo.git
git push -u origin master

git log --graph --pretty=oneline -5
git reset --hard HEAD^
git diff HEAD -- readme.txt
git checkout --readme.txt
git reset HEAD readme.txt
git push origin master
git push origin dev
git checkout -b dev
git merge --no-ff -m "something" dev
git branch -d dev
git stash
git stash pop
git stash apply
